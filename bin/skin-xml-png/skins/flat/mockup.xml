				<setting name="titleactivecolor" value="ffffff"/>
				<setting name="titleovercolor" value="ffffff"/>
				<setting name="titlesize" value="13"/>
				<setting name="titleweight" value="normal"/>
			</settings>
			<elements>
				<element name="divider" src="divider.png"/>
